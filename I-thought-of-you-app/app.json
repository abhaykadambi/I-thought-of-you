{
  "expo": {
    "name": "ITY",
    "slug": "i-thought-of-you",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "ithoughtofyou",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f5ee"
    },
    "expo-splash-screen": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f5ee"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ithoughtofyou.app",
      "usesAppleSignIn": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSContactsUsageDescription": "This app uses your contacts to help you find and connect with friends who are also using the app. We only access contacts to suggest friends and do not store or share your contact information.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to allow you to select and upload profile pictures. We only access photos that you specifically choose to upload.",
        "NSCameraUsageDescription": "This app needs access to your camera to allow you to take photos for your thoughts. We only access the camera when you choose to take a photo."
      }
    },
    "android": {
      "package": "com.ithoughtofyou.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "NOTIFICATIONS",
        "VIBRATE",
        "CAMERA"
      ]
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#ffffff",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "New Thought"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "35bc5d75-73b4-4250-b2d1-470d61a7279d"
      }
    },
    "owner": "pumpkinapie"
  }
}
